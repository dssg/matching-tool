{
    "name": "Jail Booking Charges",
    "fields": [
        {
            "name": "internal_person_id",
            "type": "string"
        },
        {
            "name": "internal_event_id",
            "type": "string"
        },
        {
            "name": "internal_charge_id",
            "type": "string",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "inmate_number",
            "type": "string"
        },
        {
            "name": "full_name",
            "type": "string"
        },
        {
            "name": "prefix",
            "type": "string"
        },
        {
            "name": "first_name",
            "type": "string"
        },
        {
            "name": "middle_name",
            "type": "string"
        },
        {
            "name": "last_name",
            "type": "string"
        },
        {
            "name": "suffix",
            "type": "string"
        },
        {
            "name": "dob",
            "type": "string",
            "constraints": {
                "partial_dob": true
            }
        },
        {
            "name": "ssn",
            "type": "string",
            "constraints": {
                "pattern": "[0-9X]{9}"
            }
        },
        {
            "name": "ssn_hash",
            "type": "string",
            "constraints": {
                "pattern": "[0-9a-f]*$"
            }
        },
        {
            "name": "ssn_bigrams",
            "type": "string",
            "constraints": {
                "pattern": "([0-9a-f]*,){9}[0-9a-f]*"
            }
        },
        {
            "name": "fingerprint_id",
            "type": "string"
        },
        {
            "name": "dmv_number",
            "type": "string"
        },
        {
            "name": "dmv_state",
            "type": "string"
        },
        {
            "name": "additional_id_name",
            "type": "string"
        },
        {
            "name": "additional_id_number",
            "type": "string"
        },
        {
            "name": "race",
            "type": "string",
            "constraints": {
                "enum_maybe_list": ["W", "B", "A", "I", "P", "H", "O", "D", "R", "N"]
            }
        },
        {
            "name": "ethnicity",
            "type": "string",
            "constraints": {
                "enum": [
                    "HISPANIC",
                    "NOT HISPANIC",
                    "INMATE DOESN'T KNOW",
                    "INMATE REFUSED",
                    "DATA NOT COLLECTED"
                ]
