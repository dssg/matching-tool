            "constraints": {
                "enum": [
                    "F",
                    "M",
                    "C",
                    "T",
                    "I",
                    "O"
                ]
            }
        },
        {
            "name": "offense_category",
            "type": "string"
        },
        {
            "name": "charge_class",
            "type": "string"
        },
        {
            "name": "bond_amount",
            "type": "number"
        },
        {
            "name": "source_name",
            "type": "string"
        },
        {
            "name": "created_date",
            "type": "string",
            "constraints": {
