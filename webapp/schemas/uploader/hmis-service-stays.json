{
	"name": "HMIS Service Stays",
    "fields": [
        {
            "name": "internal_person_id",
            "type": "string",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "secondary_person_id",
            "type": "string"
        },
        {
            "name": "internal_event_id",
            "type": "string",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "full_name",
            "type": "string"
        },
        {
            "name": "prefix",
            "type": "string"
        },
        {
            "name": "first_name",
            "type": "string"
        },
        {
            "name": "middle_name",
            "type": "string"
        },
        {
            "name": "last_name",
            "type": "string"
        },
        {
            "name": "suffix",
            "type": "string"
        },
        {
            "name": "name_data_quality",
            "type": "string",
			"constraints": {
				"enum": [
					"FULL NAME REPORTED",
					"PARTIAL, STREET NAME, OR CODE NAME REPORTED",
					"CLIENT DOESN'T KNOW",
					"CLIENT REFUSED",
					"DATA NOT COLLECTED"
				]
			}
        },
        {
            "name": "dob",
            "type": "string",
			"constraints": {
				"partial_dob": true
			}
        },
        {
            "name": "dob_type",
            "type": "string",
			"constraints": {
				"enum": [
					"FULL DOB REPORTED",
					"APPROXIMATE OR PARTIAL DOB REPORTED",
					"CLIENT DOESN'T KNOW",
					"CLIENT REFUSED",
					"DATA NOT COLLECTED"
				]
			}
        },
		{
			"name": "ssn",
			"type": "string",
			"constraints": {
				"pattern": "[0-9X]{9}"
			}
		},
		{
			"name": "ssn_hash",
			"type": "string",
			"constraints": {
				"pattern": "[0-9a-f]*$"
			}
		},
		{
			"name": "ssn_bigrams",
			"type": "string",
			"constraints": {
				"pattern": "([0-9a-f]*,){9}[0-9a-f]*"
			}
		},
		{
			"name": "ssn_data_quality",
			"type": "string",
			"constraints": {
				"enum": [
					"FULL SSN REPORTED",
					"APPROXIMATE OR PARTIAL SSN REPORTED",
					"CLIENT DOESN'T KNOW",
					"CLIENT REFUSED",
					"DATA NOT COLLECTED"
				]
			}
		},
        {
            "name": "dmv_number",
            "type": "string"
        },
        {
            "name": "dmv_state",
            "type": "string",
			"constraints": {
				"maxLength": 2
			}
        },
        {
            "name": "additional_id_number",
            "type": "string"
        },
        {
            "name": "additional_id_name",
            "type": "string"
        },
        {
            "name": "race",
            "type": "string",
            "constraints": {
				"enum_maybe_list": ["W", "B", "A", "I", "P", "H", "O", "D", "R", "N"]
            }
        },
        {
            "name": "secondary_race",
            "type": "string",
            "constraints": {
				"enum_maybe_list": ["W", "B", "A", "I", "P", "H", "O", "D", "R", "N"]
            }
        },
        {
            "name": "ethnicity",
            "type": "string",
			"constraints": {
				"enum": ["HISPANIC", "NOT HISPANIC", "CLIENT DOESN'T KNOW", "CLIENT REFUSED", "DATA NOT COLLECTED"]
			}
        },
		{
			"name": "sex",
			"type": "string",
			"constraints": {
				"enum": ["F", "M", "MT", "FT", "O", "D", "R", "N"]
			}
		},
		{
			"name": "street_address",
			"type": "string"
		},
		{
