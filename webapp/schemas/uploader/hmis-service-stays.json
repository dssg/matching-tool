			"type": "string"
		},
		{
			"name": "state",
			"type": "string"
		},
		{
			"name": "postal_code",
			"type": "string"
		},
		{
			"name": "county",
			"type": "string"
		},
		{
			"name": "country",
			"type": "string"
		},
		{
			"name": "address_data_quality",
			"type": "string",
			"constraints": {
				"enum": [
					"FULL ADDRESS REPORTED",
					"FULL OR PARTIAL ZIP CODE REPORTED",
					"INCOMPLETE OR ESTIMATED ADDRESS REPORTED",
					"CLIENT DOESN'T KNOW",
					"CLIENT REFUSED",
					"DATA NOT COLLECTED"
				]
			}
		},
		{
			"name": "veteran_status",
			"type": "string",
			"constraints": {
				"enum": [
					"NO",
					"YES",
					"CLIENT DOESN'T KNOW",
					"CLIENT REFUSED",
					"DATA NOT COLLECTED"
				]
			}
		},
